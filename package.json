{
  "name": "sg-heatmap",
  "version": "2.1.1",
  "description": "Open-source all-in-one Swiss Army knife tool for creating Choropleth maps",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel src --out-dir dist",
    "extract": "babel-node src/extract.js",
    "prepublish": "npm run build && npm run test",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongjun21/sg-heatmap.git"
  },
  "keywords": [
    "data",
    "visualization",
    "heatmap"
  ],
  "author": "Yong Jun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yongjun21/sg-heatmap/issues"
  },
  "homepage": "https://github.com/yongjun21/sg-heatmap#readme",
  "dependencies": {
    "chroma-js": "^1.2.1",
    "lodash": "^4.15.0",
    "polyline": "^0.2.0",
    "proj4": "^2.3.15"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "html2json": "^1.0.2",
    "standard": "^8.0.0",
    "xml2js": "^0.4.17"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
